<ehcache>
    <diskstore path="java.io.tmpdir"/>

    <defaultCache
        maxElementsInMemory="100000"
        eternal="false"
        overflowToDisk="true"
        timeToLiveSeconds="60"/>

    <!-- 1 hour cache -->
    <cache
        name="elCoreElements"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="true"
        timeToLiveSeconds="3600"/>

    <!-- 1 hour cache -->
    <cache
        name="elTranslateElements"
        maxElementsInMemory="10000000"
        eternal="false"
        overflowToDisk="true"
        timeToLiveSeconds="3600"/>


</ehcache>