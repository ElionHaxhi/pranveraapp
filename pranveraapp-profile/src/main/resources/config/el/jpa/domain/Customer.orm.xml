<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/ns/persistence/orm_2_0.xsd"
                 version="2.0">
    <named-query name="EL_READ_CUSTOMER_BY_USER_NAME">
        <query>SELECT customer FROM com.pranveraapp.profile.domain.Customer customer
            WHERE customer.username = :username
        </query>
    </named-query>
    <named-query name="EL_READ_ALL_CUSTOMER">
        <query>SELECT customer FROM com.pranveraapp.profile.domain.Customer customer
        </query>
    </named-query>

</entity-mappings>